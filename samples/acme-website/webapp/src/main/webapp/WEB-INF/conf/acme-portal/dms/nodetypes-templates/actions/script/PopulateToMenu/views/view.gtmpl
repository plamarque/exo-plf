    <td>
  		<%if(node.hasProperty("exo:page")){%>
  			<%=node.getProperty("exo:page").getString()%>
  		<%}%>	
 		</td>
  </tr>
  <tr>
    <td style="width:25%"><%=_ctx.appRes("PopulateToMenu.view.label.childrenPage")%></td>
