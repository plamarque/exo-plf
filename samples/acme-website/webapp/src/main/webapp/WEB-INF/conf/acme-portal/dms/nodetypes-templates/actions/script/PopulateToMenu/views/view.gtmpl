  		<%if(node.hasProperty("exo:childrenPage")){%>
  			<%=node.getProperty("exo:childrenPage").getString()%>
  		<%}%>	
 		</td>
  </tr>
  </tr>
  <tr>
    <td style="width:25%"><%=_ctx.appRes("PopulateToMenu.view.label.lifecycle")%></td>
