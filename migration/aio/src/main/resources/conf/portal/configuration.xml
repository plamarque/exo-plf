<?xml version="1.0" encoding="ISO-8859-1"?>
<configuration>
	<component>
		<key>org.exoplatform.platform.migration.common.component.ContainerParamExtractor</key>
		<type>org.exoplatform.platform.migration.aio.component.ContainerParamExtractorImpl</type>
	</component>
	<component>
		<key>org.exoplatform.platform.migration.common.component.logger</key>
		<type>org.exoplatform.platform.migration.aio.component.LoggerImpl</type>
	</component>
	<component>
		<type>org.exoplatform.platform.migration.aio.rest.MarshallConfigurationREST</type>
	</component>
	<component>
		<type>org.exoplatform.platform.migration.aio.rest.AIOBackupMigrationREST</type>
	</component>
	<component>
		<type>org.exoplatform.platform.migration.aio.backup.AIOBackupMigrationService</type>
		<init-params>
			<value-param>
				<name>delete.wcm.services.log.nodes</name>
				<value>true</value>
			</value-param>
		</init-params>
	</component>
	<external-component-plugins>
		<target-component>org.exoplatform.platform.migration.common.component.MarshallConfigurationService</target-component>
		<component-plugin>
			<name>application.registry.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.ApplicationRegistryHandler</type>
		</component-plugin>
		<component-plugin>
			<name>portal.configuration.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.UserPortalConfigHandler</type>
		</component-plugin>
		<component-plugin>
			<name>nodeType.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.NodeTypeManagerConfigHandler</type>
		</component-plugin>
		<component-plugin>
			<name>nodeType.templates.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.NodeTypeTemplateHandler</type>
		</component-plugin>
		<component-plugin>
			<name>application.templates.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.ApplicationTemplatesHandler</type>
		</component-plugin>
		<component-plugin>
			<name>application.templates.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.MetadataTemplateHandler</type>
		</component-plugin>
		<component-plugin>
			<name>views.templates.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.ManageViewsHandler</type>
		</component-plugin>
		<component-plugin>
			<name>scripts.templates.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.GroovyScriptHandler</type>
		</component-plugin>
		<component-plugin>
			<name>drives.templates.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.ManageDriveHandler</type>
		</component-plugin>
		<component-plugin>
			<name>queries.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.QueryHandler</type>
		</component-plugin>
		<component-plugin>
			<name>folksonomy.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.FolksonomyHandler</type>
		</component-plugin>
		<component-plugin>
			<name>taxonomy.handler</name>
			<set-method>addHandler</set-method>
			<type>org.exoplatform.platform.migration.aio.handler.impl.TaxonomyHandler</type>
		</component-plugin>
	</external-component-plugins>
</configuration>