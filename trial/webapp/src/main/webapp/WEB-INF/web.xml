<?xml version="1.0" encoding="ISO-8859-1" ?> 
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                 "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>trial</display-name>
	<servlet>
		<servlet-name>UnlockServlet</servlet-name>
		<servlet-class>org.exoplatform.trial.UnlockServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PingBackServlet</servlet-name>
		<servlet-class>org.exoplatform.trial.PingBackServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PingBackServlet</servlet-name>
		<url-pattern>/PingBackServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UnlockServlet</servlet-name>
		<url-pattern>/UnlockServlet</url-pattern>
	</servlet-mapping>
</web-app>