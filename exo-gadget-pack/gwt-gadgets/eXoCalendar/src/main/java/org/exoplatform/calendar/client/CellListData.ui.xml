<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:org.exoplatform.calendar.client">
	<ui:style>
    .scrollable {
      height: 200px;
      width: 250px;
      border: 1px solid #ccc;
      text-align: left;
    }
    
    .contactFormCell {
      padding-left: 20px;
    }
  </ui:style>

  <g:HTMLPanel>
    <table>
      <tr>
        <td
          valign='top'
          align='center'>
          <s:ShowMorePagerPanel
            addStyleNames='{style.scrollable}'
            ui:field='pagerPanel' />
          
        </td>
      </tr>
    </table>
  </g:HTMLPanel>
</ui:UiBinder> 