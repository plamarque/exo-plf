<%
	import org.exoplatform.platform.webui.container.UIPinContainer;

	if(uicomponent.hasPermission()) {
		if(uicomponent.getChild(UIPinContainer.class) == null) {
			uicomponent.addChild(UIPinContainer.class, null, null);
		}
		uicomponent.renderChild(UIPinContainer.class);
	} else { %>
		<div class='ProtectedContent'>
			<%=_ctx.appRes("UIPortlet.label.protectedContent")%>
		</div>
<%	} %>